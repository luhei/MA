@article{Grossmann2014,
abstract = {Are people wiser when reflecting on other people's problems$\backslash$ncompared with their own? If so, does self-distancing eliminate$\backslash$nthis asymmetry in wise reasoning? In three experiments (N =$\backslash$n693), participants displayed wiser reasoning (i.e.,$\backslash$nrecognizing the limits of their knowledge and the importance$\backslash$nof compromise and future change, considering other people's$\backslash$nperspectives) about another person's problems compared with$\backslash$ntheir own. Across Studies 2 and 3, instructing individuals to$\backslash$nself-distance (rather than self-immerse) eliminated this$\backslash$nasymmetry. Study 3 demonstrated that each of these effects was$\backslash$ncomparable for younger (20-40 years) and older (60-80 years)$\backslash$nadults. Thus, contrary to the adage ``with age comes wisdom,''$\backslash$nour findings suggest that there are no age differences in wise$\backslash$nreasoning about personal conflicts, and that the effects of$\backslash$nself-distancing generalize across age cohorts. These findings$\backslash$nhighlight the role that self-distancing plays in allowing$\backslash$npeople to overcome a pervasive asymmetry that characterizes$\backslash$nwise reasoning.},
author = {Grossmann, Igor and Kross, Ethan},

journal = {Psychological Science},
keywords = {aging,close relationships,emotional intelligence,open data,open materials,perspective taking,psychological distance,self-distance,wisdom},
pmid = {24916084},
title = {{Exploring Solomon's Paradox: Self-Distancing Eliminates the Self-Other Asymmetry in Wise Reasoning About Close Relationships in Younger and Older Adults}},

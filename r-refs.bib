keywords = {addiction,need to belong,neuroimaging,self-awareness,social neuroscience,theory of mind},
month = {jan},
number = {1},
pages = {363--390},
publisher = {Annual Reviews},
title = {{Neuroscience of Self and Self-Regulation}},
url = {http://www.annualreviews.org/doi/10.1146/annurev.psych.121208.131616},

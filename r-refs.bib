isbn = {0269-9931, 0269-9931},
issn = {02699931},
journal = {Cognition {\&} emotion},
keywords = {COGNITION,Decision Making,Decision-making,Emotions,Humans,Judgement,Judgment,Meta-analysis,Publication Bias/statistics {\&} numerical data},
language = {eng TS - PubMed},
number = {8},

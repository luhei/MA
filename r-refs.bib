file = {:C$\backslash$:/Users/Lutz/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Moser et al. - Unknown - Neural markers of positive reappraisal and their associations with trait reappraisal and worry.pdf:pdf},
issn = {1939-1846},
journal = {Journal of Abnormal Psychology},
keywords = {cognitive change,emotion regulation,event-related potentials,late positive potential,reappraisal,worry},
number = {1},
pages = {91--105},
title = {{Neural markers of positive reappraisal and their associations with trait reappraisal and worry.}},
url = {http://psycnet.apa.org/fulltext/2014-09291-010.html http://doi.apa.org/getdoi.cfm?doi=10.1037/a0035817},
volume = {123},
